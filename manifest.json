{
  "manifest_version": 3,
  "name": "OneNOTE",
  "version": "1.0",
  "description": "A simple note-taking extension.",
  "permissions": ["activeTab"],
  "author": "Yash Bhidawe",
  "browser_action": {
    "default_popup": "index.html"
   
  },
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ]
}
